<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/score"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/shape_oval"
        android:backgroundTint="?attr/colorTertiary"
        android:gravity="center"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="30"
        android:textSize="35dp" />

    <TextView
        android:id="@+id/total"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/score"
        android:layout_alignBottom="@+id/score"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="/ 30"
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/orientation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/barChart2"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:layout_toStartOf="@+id/barChart"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="지남력"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <ProgressBar
        android:id="@+id/barChart"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="12dp"
        android:layout_below="@+id/score"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="10dp"
        android:progress="30"
        android:progressDrawable="@drawable/progressbar" />

    <TextView
        android:id="@+id/ori_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/barChart2"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="8dp"
        android:layout_toEndOf="@+id/barChart"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="5/5"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/memory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/barChart3"
        android:layout_toStartOf="@+id/barChart"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="기억력"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <ProgressBar
        android:id="@+id/barChart2"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="12dp"
        android:layout_below="@+id/barChart"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:progress="50"
        android:progressDrawable="@drawable/progressbar" />

    <TextView
        android:id="@+id/mem_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/barChart3"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="8dp"
        android:layout_toEndOf="@+id/barChart"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="10/10"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/attention"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/barChart4"
        android:layout_toStartOf="@+id/barChart"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="주의력"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <ProgressBar
        android:id="@+id/barChart3"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="12dp"
        android:layout_below="@+id/barChart2"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:progress="20"
        android:progressDrawable="@drawable/progressbar" />

    <TextView
        android:id="@+id/att_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/barChart4"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="8dp"
        android:layout_toEndOf="@+id/barChart"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="3/3"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/spacetime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/barChart5"
        android:layout_toStartOf="@+id/barChart"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="시공간"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <ProgressBar
        android:id="@+id/barChart4"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="12dp"
        android:layout_below="@+id/barChart3"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:progress="50"
        android:progressDrawable="@drawable/progressbar" />

    <TextView
        android:id="@+id/spa_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/barChart5"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="8dp"
        android:layout_toEndOf="@+id/barChart"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="2/2"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/execution"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/barChart6"
        android:layout_toStartOf="@+id/barChart"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="집행기능"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <ProgressBar
        android:id="@+id/barChart5"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="12dp"
        android:layout_below="@+id/barChart4"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:progress="70"
        android:progressDrawable="@drawable/progressbar" />

    <TextView
        android:id="@+id/exe_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/barChart6"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="8dp"
        android:layout_toEndOf="@+id/barChart"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="6/6"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/language"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/result_cardView"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="10dp"
        android:layout_toStartOf="@+id/barChart"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="언어기능"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <ProgressBar
        android:id="@+id/barChart6"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="12dp"
        android:layout_below="@+id/barChart5"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="25dp"
        android:progress="30"
        android:progressDrawable="@drawable/progressbar" />

    <TextView
        android:id="@+id/lan_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/result_cardView"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="24dp"
        android:layout_toEndOf="@+id/barChart"
        android:fontFamily="@font/jejugothic"
        android:letterSpacing="0.04"
        android:text="4/4"
        android:textColor="?attr/colorSecondaryVariant"
        android:textSize="12sp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/endBtn"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_below="@+id/result_cardView"
        android:layout_alignStart="@+id/result_cardView"
        android:layout_alignEnd="@+id/result_cardView"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="30dp"
        android:background="@drawable/round_button"
        android:backgroundTint="?attr/colorPrimary"
        android:elevation="10dp"
        android:fontFamily="@font/jejugothic"
        android:gravity="center"
        android:letterSpacing="0.5"
        android:text="확 인"
        android:textColor="?attr/colorOnPrimary"
        android:textSize="16sp" />

    <androidx.cardview.widget.CardView
        android:id="@+id/result_cardView"
        android:layout_width="270dp"
        android:layout_height="170dp"
        android:layout_marginBottom="20dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/barChart6"
        app:cardElevation="10dp">

        <TextView
            android:id="@+id/result_text"
            android:layout_width="270dp"
            android:layout_height="170dp"
            android:gravity="center"
            android:padding="10dp"
            android:fontFamily="@font/jejugothic"
            android:letterSpacing="0.04"
            android:lineSpacingExtra="10dp"
            android:textSize="13sp"
            android:text="여기는 진단 결과 설명이 나오는 부분.\n
여기는 진단 결과 설명이 나오는 부분.\n여기는 진단 결과 설명이 나오는 부분.\n
여기는 진단 결과 설명이 나오는 부분.\n여기는 진단 결과 설명이 나오는 부분." />
    </androidx.cardview.widget.CardView>
</RelativeLayout>